{
  "name": "hcl-commerce-react-store",
  "version": "9.1.12",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'dist/emerald/static/js/*.js' --html tools/source-map-explorer.html",
    "start": "node tools/scripts/start.js",
    "dev": "craco start",
    "predev": "rimraf .eslintcache",
    "mock": "node tools/scripts/start.js --mode mock",
    "preview": "node tools/scripts/start.js --mode preview",
    "mockApi": "node tools/mockServer.js",
    "build": "node tools/scripts/build.js",
    "prebuild": "rimraf .eslintcache && rimraf dist && rimraf src/_dev",
    "bundle": "craco build --verbose",
    "test": "craco test",
    "test-dev": "run-p mockApi test:dev --env=CI",
    "test:dev": "react-scripts test --coverage --collectCoverageFrom=/**/test.{ts,js,jsx,tsx}",
    "generate-rest": "node tools/scripts/generateRestService.js",
    "eject": "react-scripts eject",
    "pretty-quick": "pretty-quick",
    "generateAsset": "rimraf public && node tools/scripts/copyStoreAsset.js",
    "cleanupBundle": "node tools/scripts/cleanupBundle.js",
    "lint": "eslint --max-warnings 0 ."
  },
  "dependencies": {
    "@date-io/date-fns": "2.14.0",
