{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "declaration": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6", "dom", "es2016", "es2017", "ESNext", "DOM.Iterable"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "allowJs": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true
  },
  "references": [
    {
      "path": "./react-store"
    },
    {
      "path": "./packages/react-component"
    },
    {
      "path": "./packages/typescript-axios-approvals"
    },
    {
      "path": "./packages/typescript-axios-es"
    },
    {
      "path": "./packages/typescript-axios-transaction"
    },
    {
      "path": "./packages/utils"
    }
  ]
}
